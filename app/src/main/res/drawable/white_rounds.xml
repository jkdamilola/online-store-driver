<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true" android:state_pressed="true">
        <shape>
            <solid android:color="#ffffffff" />
            <stroke android:width="1.0dip" android:color="#ffeaeaee" />
            <corners android:radius="10.0dip" />
        </shape>
    </item>
    <item android:state_focused="false" android:state_pressed="true">
        <shape>
            <solid android:color="#ffffffff" />
            <stroke android:width="1.0dip" android:color="#ffeaeaee" />
            <corners android:radius="10.0dip" />
        </shape>
    </item>
    <item android:state_focused="true" android:state_pressed="false">
        <shape>
            <solid android:color="#ffffffff" />
            <stroke android:width="1.0dip" android:color="#ffeaeaee" />
            <corners android:radius="10.0dip" />
        </shape>
    </item>
    <item android:state_focused="false" android:state_pressed="false">
        <shape>
            <gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:angle="270.0" android:centerColor="#ffffffff" />
            <stroke android:width="1.0dip" android:color="#ffeaeaee" />
            <corners android:radius="10.0dip" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <padding android:left="0.0dip" android:top="0.0dip" android:right="0.0dip" android:bottom="0.0dip" />
        </shape>
    </item>
</selector>